{
  "name": "@binary-format/binary-format",
  "version": "0.0.9",
  "description": "a typescript library for reading and writing binary files to and from plain javascript objects",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rimraf dist/ && rollup -c",
    "cover": "npm run test -- --coverage",
    "format": "prettier --write .",
    "lint": "eslint .",
    "prepublish": "npm run lint && npm run cover && npm run build",
    "test": "jest --no-cache",
    "watch": "npm run cover -- --watchAll"
  },
  "homepage": "https://github.com/skratchdot/binary-format/",
  "repository": {
    "type": "git",
    "url": "git://github.com/skratchdot/binary-format.git"
  },
  "bugs": {
    "url": "https://github.com/skratchdot/binary-format/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-babel": "^5.3.0",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "jest": "^27.2.5",
    "jest-buffer-snapshot-serializer": "^1.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-license": "^2.6.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "bit-buffer": "^0.2.5",
    "smart-buffer": "^4.2.0"
  },
  "keywords": [
    "arraybuffer",
    "arrays",
    "binary",
    "bit",
    "bits",
